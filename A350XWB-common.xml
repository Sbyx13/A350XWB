    <menubar>
      <default>
        <menu n="100">
          <label>Airbus A350XWB</label>
          <item>
            <label>Autostart</label>
            <binding>
              <command>nasal</command>
              <script>A350XWB.enginesystem.autostart();</script>
            </binding>
          </item>
          <item>
            <label>Select livery</label>
            <binding>
              <command>nasal</command>
              <script>aircraft.livery.dialog.toggle()</script>
            </binding>
          </item>
          <!--
					<item>
						<name>autostart</name>
						<label>Autostart Aircraft Engines</label>
						<binding>
							<command>property-toggle</command>
							<property>sim/model/start-idling</property>
						</binding>
					</item>
					<item>
						<label>Select Aircraft Livery</label>
						<binding>
							<command>nasal</command>
							<script>
							 aircraft.livery.dialog.open()
							</script>
						</binding>
					</item>

-->
          <item>
            <label>Pushback</label>
            <binding>
              <command>dialog-show</command>
              <dialog-name>pushback</dialog-name>
            </binding>
          </item>
          <item>
            <label>Autopilot Dialog</label>
            <binding>
              <command>nasal</command>
              <script>var ap_dialog = gui.Dialog.new("sim/gui/dialogs/ap_dialog/dialog", "Aircraft/A350XWB/Systems/autopilot-dlg.xml");
							 ap_dialog.open();</script>
            </binding>
          </item>
          <item>
            <label>Radio Stack</label>
            <binding>
              <command>nasal</command>
              <script>dialogs.Radio.open()</script>
            </binding>
          </item>
          <item>
            <label>Virtual Co-pilot Settings</label>
            <binding>
              <command>nasal</command>
              <script>var copilotdialog = gui.Dialog.new("sim/gui/dialogs/copilot/dialog", "Aircraft/A350XWB/Systems/copilot-dlg.xml");
							 copilotdialog.open();</script>
            </binding>
          </item>
          <item>
            <label>Import/Export Flightplan</label>
            <binding>
              <command>nasal</command>
              <script>var flightplan = gui.Dialog.new("sim/gui/dialogs/flightplan/dialog", "Aircraft/A350XWB/Systems/flightplan-dlg.xml");
							 flightplan.open();</script>
            </binding>
          </item>
          <item>
            <name>pushback</name>
            <label>Request for Pushback</label>
            <binding>
              <command>dialog-show</command>
              <dialog-name>pushback</dialog-name>
            </binding>
          </item>
          <item>
            <name>lights</name>
            <label>Control Aircraft Lights</label>
            <binding>
              <command>nasal</command>
              <script>var lights = gui.Dialog.new("sim/gui/dialogs/lights/dialog", "Aircraft/A350XWB/Systems/lights-dlg.xml");
							 lights.open();</script>
            </binding>
          </item>
          <item>
            <name>announcements</name>
            <label>Make Announcements</label>
            <binding>
              <command>nasal</command>
              <script>var announce = gui.Dialog.new("sim/gui/dialogs/announcements/dialog", "Aircraft/A350XWB/Systems/announcements-dlg.xml");
							 announce.open();</script>
            </binding>
          </item>
          <item>
            <label>Simulate System Failures</label>
            <binding>
              <command>nasal</command>
              <script>var failures = gui.Dialog.new("sim/gui/dialogs/failures/dialog", "Aircraft/A350XWB/Systems/failures-dlg.xml");
							 failures.open();</script>
            </binding>
          </item>
          <item>
            <label>Ground Services Controls</label>
            <binding>
              <command>nasal</command>
              <script>var ground_services = gui.Dialog.new("sim/gui/dialogs/ground_services/dialog", "Aircraft/A350XWB/Systems/ground_services-dlg.xml");
							 ground_services.open();</script>
            </binding>
          </item>
          <item>
            <label>Fuel Truck Controls</label>
            <binding>
              <command>nasal</command>
              <script>var fuel_truck = gui.Dialog.new("sim/gui/dialogs/fuel_truck/dialog", "Aircraft/A350XWB/Systems/fuel_truck.xml");
							 fuel_truck.open();</script>
            </binding>
          </item>
          <item>
            <name>dualcontrol</name>
