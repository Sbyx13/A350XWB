              <script>var ground_services = gui.Dialog.new("sim/gui/dialogs/ground_services/dialog", "Aircraft/A350XWB/Systems/ground_services-dlg.xml");
							 ground_services.open();</script>
            </binding>
          </item>
          <item>
            <label>Fuel Truck Controls</label>
            <binding>
              <command>nasal</command>
              <script>var fuel_truck = gui.Dialog.new("sim/gui/dialogs/fuel_truck/dialog", "Aircraft/A350XWB/Systems/fuel_truck.xml");
