<?xml version="1.0"?>

<PropertyList>

<path>light-flare.ac</path>


<animation>
	<type>light</type>
	<light-type>spot</light-type>
	<name>LightNavStbd</name>
	<object-name>LightVolNavStbd</object-name>
	<position>
		<x> 0 </x>
		<y>0 </y>
		<z> 10 </z>
	</position>
	<direction>
		<x>0</x>
		<y>0</y>
		<z>-1.0</z>
	</direction>
	<ambient>
		<r>0.03</r>
		<g>0.03</g>
		<b>0.03</b>
		<a>1.0</a>
	</ambient>
	<diffuse>
		<r>1.0</r>
		<g>0.2</g>
		<b>0.2</b>
		<a>1.0</a>
	</diffuse>
	<specular>
		<r>0.7</r>
		<g>0.7</g>
		<b>0.7</b>
		<a>1.0</a>
	</specular>
	<attenuation>
		<c>1.0</c>
		<l>0.05</l>
		<q>0.1</q>
	</attenuation>
	<exponent>10</exponent>
	<near-m>0.001</near-m>
	<far-m>79</far-m>
	<cutoff>110</cutoff>
</animation>

<animation>
	<type>select</type>
	<object-name>LightNavStbd</object-name>
	<condition>
		<and>
			<equals>
				<property>/controls/lighting/nav-lights</property>
				<value>1</value>
			</equals>
			<property>/sim/rendering/rembrandt/enabled</property> 
		</and>
	</condition>
</animation>



</PropertyList>
